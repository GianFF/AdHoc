<% if @escrito.type.eql?('Demanda') %>
    <%= link_to 'Presentar', expediente_presentar_demanda_path(@expediente.id, @escrito.id), method: :put, class: 'btn btn-warning boton_presentar', disabled: @escrito.fue_presentado?, data: { confirm: 'Esta accion no se podra revertir', 'confirm-button-text': 'Continuar', 'confirm-button-color': '#5cb85c', 'cancel-button-text': 'Cancelar', 'cancel-button-color': '#ec971f', 'sweet-alert-type': 'warning', text: 'Una vez presentado el escrito no podra ser editado.'} %>
