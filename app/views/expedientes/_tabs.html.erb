<ul class="nav nav-tabs">
  <li role="presentation" class="active">
    <a href="#" id="tabs__tab-activa">
      <% if @expediente &&  @expediente.actor && @expediente.demandado %>
          <%= @expediente.titulo %>
      <% else %>
          Nuevo expediente
      <% end %>
    </a>
  </li>
  <% expedientes_que_no_son(@expediente, @cliente.expedientes).each do |expediente| %>
      <li role="presentation" >
        <%= link_to expediente.titulo, cliente_expediente_path(@cliente.id, expediente.id) %>
      </li>
  <% end %>
</ul>
<%= render 'shared/tabs_contenedor' do %>
    <%= yield %>
<% end %>